ents/ApprovedCourses.js","original":{"line":74,"column":36}},{"generated":{"line":122,"column":25},"source":"components/ApprovedCourses.js","original":{"line":73,"column":32}},{"generated":{"line":122,"column":26},"source":"components/ApprovedCourses.js","original":{"line":69,"column":28}},{"generated":{"line":122,"column":27},"source":"components/ApprovedCourses.js","original":{"line":68,"column":60}},{"generated":{"line":123,"column":0},"source":"components/ApprovedCourses.js","original":{"line":68,"column":60}},{"generated":{"line":123,"column":7},"source":"components/ApprovedCourses.js","original":{"line":68,"column":25}},{"generated":{"line":123,"column":8},"source":"components/ApprovedCourses.js","original":{"line":67,"column":20}},{"generated":{"line":123,"column":9},"source":"components/ApprovedCourses.js","original":{"line":66,"column":16}},{"generated":{"line":123,"column":10},"source":"components/ApprovedCourses.js","original":{"line":64,"column":12}},{"generated":{"line":123,"column":11},"source":"components/ApprovedCourses.js","original":{"line":63,"column":8}},{"generated":{"line":124,"column":0},"source":"components/ApprovedCourses.js","original":{"line":83,"column":5}},{"name":"Component","generated":{"line":128,"column":2},"source":"components/ApprovedCourses.js","original":{"line":5,"column":30}},{"name":"ApprovedCourses","generated":{"line":130,"column":15},"source":"components/ApprovedCourses.js","original":{"line":85,"column":15}}],"sources":{"components/ApprovedCourses.js":"import React,{Component} from 'react';\r\nimport{Container,ListGroup,ListGroupItem} from \"reactstrap\";\r\nimport LAppNavbar from \"./LHome\";\r\n\r\nclass ApprovedCourses extends Component{\r\n    constructor(props){\r\n        super(props);\r\n        this.state = {\r\n            testCourses:[]\r\n        }\r\n    }\r\n\r\n    //handle Submit\r\n    handleSubmit(e) {\r\n\r\n        const joinCourse = {\r\n            \"studentId\": e.upload.studentId,\r\n            \"courseId\": e.upload.courseId\r\n\r\n        };\r\n        alert(joinCourse.studentId + \", \" + joinCourse.courseId);\r\n\r\n        fetch('http://localhost:3000/api/students/join-a-course', {\r\n            method: 'POST',\r\n            headers: {\r\n                'content-type': 'application/json'\r\n            },\r\n            body: JSON.stringify(joinCourse)\r\n        }).then(response => {\r\n            console.log(response)\r\n        }).catch(err => {\r\n            console.log(err);\r\n        });\r\n    }\r\n\r\n    //handle On submit\r\n    handleOnSubmit(e){\r\n        e.preventDefault();\r\n\r\n        const joinCourse_details = {\r\n            studentId: document.getElementById('studentId').value,\r\n            courseId: document.getElementById('courseId').value\r\n        };\r\n        this.handleSubmit({joinCourse: joinCourse_details});\r\n    }\r\n\r\n    onSubmit() {\r\n        alert(\"Are you sure?\");\r\n    }\r\n    componentDidMount() {\r\n        fetch('http://localhost:3000/api/testcourses/get-test-courses')\r\n            .then( res => {\r\n                return res.json();\r\n            })\r\n            .then(json => {\r\n                console.log(json);\r\n                this.setState({\r\n                    testCourses:json\r\n                })\r\n            })\r\n    }\r\n    render(){\r\n        return(\r\n            <div>\r\n                <LAppNavbar/>\r\n                <Container>\r\n                    <ListGroup>\r\n                        {this.state.testCourses.map( courses => (\r\n                            <ListGroupItem key={courses._id}>\r\n                                <h2>{courses.name}</h2>\r\n                                <h6>Code - {courses.code}</h6>\r\n                                <h6>Code - {courses._id}</h6>\r\n                                <form onSubmit={value => this.handleOnSubmit(value)}>\r\n                                    <button type=\"submit\" onClick={this.onSubmit}>Join Course</button>\r\n                                </form>\r\n                            </ListGroupItem>\r\n                        ))}\r\n                    </ListGroup>\r\n                </Container>\r\n            </div>\r\n\r\n        );\r\n    }\r\n}\r\nexport default ApprovedCourses;"},"lineCount":null}},"error":null,"hash":"6afb9f12fd425c933b369758384c6f04","cacheData":{"env":{}}}